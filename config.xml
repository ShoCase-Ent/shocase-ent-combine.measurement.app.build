<?xml version='1.0' encoding='utf-8'?>
<widget id="com.shocase.combine.measurements.dev" version="1.6.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Shocase Combine Measurements</name>
    <description>
        An app to record event scores and times to the Shocase server for leaderboard generation.
    </description>
    <author email="hello@digitsinmotion.com" href="http://digitsinmotion.com">
        Digits in Motion
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <hook src="hooks/prepareAngular2App.js" type="before_prepare" />
    <access origin="*" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="permissions" value="none" />
    <preference name="fullscreen" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="1000" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="windows-target-version" value="8.1" />
    <preference name="windows-phone-target-version" value="8.1" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="windows-appx-target" value="8.1-phone" />
    <platform name="ios">
        <preference name="phonegap-version" value="cli-7.1.0" />
    </platform>
    <platform name="android">
        <preference name="phonegap-version" value="cli-7.1.0" />
    </platform>
    <icon src="icon.png" />
    <icon height="1024" platform="ios" src="res/icons/ios/Icon-marketing-1024x1024.png" width="1024" />
    <icon height="334" platform="ios" src="res/icons/ios/Icon-83.5@2x.png" width="334" />
    <icon height="180" platform="ios" src="res/icons/ios/Icon-60x60@3x.png" width="180" />
    <icon height="152" platform="ios" src="res/icons/ios/Icon-76x76@2x.png" width="152" />
    <icon height="120" platform="ios" src="res/icons/ios/Icon-60x60@2x.png" width="120" />
    <icon height="120" platform="ios" src="res/icons/ios/Icon-40x40@3x.png" width="120" />
    <icon height="87" platform="ios" src="res/icons/ios/Icon-29x29@3x.png" width="87" />
    <icon height="80" platform="ios" src="res/icons/ios/Icon-40x40@2x.png" width="80" />
    <icon height="76" platform="ios" src="res/icons/ios/Icon-76x76@1x.png" width="76" />
    <icon height="60" platform="ios" src="res/icons/ios/Icon-20x20@3x.png" width="60" />
    <icon height="58" platform="ios" src="res/icons/ios/Icon-29x29@2x.png" width="58" />
    <icon height="40" platform="ios" src="res/icons/ios/Icon-20x20@2x.png" width="40" />
    <icon height="29" platform="ios" src="res/icons/ios/Icon-29x29@1x.png" width="29" />
    <icon height="20" platform="ios" src="res/icons/ios/Icon-20x20@1x.png" width="20" />
    <icon platform="android" qualifier="ldpi" src="res/icons/android/drawable-ldpi/icon.png" />
    <icon platform="android" qualifier="mdpi" src="res/icons/android/drawable-mdpi/icon.png" />
    <icon platform="android" qualifier="hdpi" src="res/icons/android/drawable-hdpi/icon.png" />
    <icon platform="android" qualifier="xhdpi" src="res/icons/android/drawable-xhdpi/icon.png" />
    <icon platform="android" qualifier="xxhdpi" src="res/icons/android/drawable-xxhdpi/icon.png" />
    <icon platform="android" qualifier="xxxhdpi" src="res/icons/android/drawable-xxxhdpi/icon.png" />
    <splash src="splash.png" />
    <splash height="480" platform="ios" src="res/screens/ios/LaunchImage~iphone-320x480.png" width="320" />
    <splash height="960" platform="ios" src="res/screens/ios/LaunchImage~iphone_640x960.png" width="640" />
    <splash height="1136" platform="ios" src="res/screens/ios/LaunchImage-568h@2x~iphone_640x1136.png" width="640" />
    <splash height="1334" platform="ios" src="res/screens/ios/LaunchImage-750@2x~iphone6-portrait_750x1334.png" width="750" />
    <splash height="1024" platform="ios" src="res/screens/ios/LaunchImage-Portrait~ipad_768x1024.png" width="768" />
    <splash height="2436" platform="ios" src="res/screens/ios/LaunchImage-1125@3x~iphoneX-portrait_1125x2436.png" width="1125" />
    <splash height="2208" platform="ios" src="res/screens/ios/LaunchImage-1242@3x~iphone6s-portrait_1242x2208.png" width="1242" />
    <splash height="2008" platform="ios" src="res/screens/ios/LaunchImage-Portrait@2x~ipad_1536x2008.png" width="1536" />
    <splash height="2048" platform="ios" src="res/screens/ios/LaunchImage-Portrait@2x~ipad_1536x2048.png" width="1536" />
    <splash height="2224" platform="ios" src="res/screens/ios/LaunchImage-Portrait@2x~ipad_1668x2224.png" width="1668" />
    <splash height="2732" platform="ios" src="res/screens/ios/LaunchImage-Portrait@2x~ipad_2048x2732.png" width="2048" />
    <splash platform="android" qualifier="ldpi" src="res/screens/android/drawable-ldpi/screen.png" />
    <splash platform="android" qualifier="mdpi" src="res/screens/android/drawable-mdpi/screen.png" />
    <splash platform="android" qualifier="hdpi" src="res/screens/android/drawable-hdpi/screen.png" />
    <splash platform="android" qualifier="xhdpi" src="res/screens/android/drawable-xhdpi/screen.png" />
    <splash platform="android" qualifier="xhdpi" src="res/screens/android/drawable-xxhdpi/screen.png" />
    <splash platform="android" qualifier="xhdpi" src="res/screens/android/drawable-xxxhdpi/screen.png" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-insomnia" spec="^4.3.0" />
    <plugin name="cordova-plugin-google-analytics" spec="^1.8.6">
        <variable name="GMS_VERSION" value="11.0.1" />
    </plugin>
    <plugin name="phonegap-plugin-barcodescanner" spec="^8.0.1">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <config-file mode="replace" parent="NSCameraUsageDescription" platform="ios">
        <string>Camera Usage is required for scanning QR Codes ad Barcodes where necessary.</string>
    </config-file>
    <config-file mode="replace" parent="NSPhotoLibraryUsageDescription" platform="ios">
        <string>Camera Usage is required for scanning QR Codes ad Barcodes where necessary.</string>
    </config-file>
    <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
        <string>Camera Usage is required for scanning QR Codes ad Barcodes where necessary</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>Camera Usage is required for scanning QR Codes ad Barcodes where necessary</string>
    </edit-config>
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="cordova-plugin-app-version" spec="^0.1.9" />
    <engine name="browser" spec="^5.0.4" />
</widget>
